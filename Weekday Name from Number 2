#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int day;
    scanf("%d",&day);
    switch(day)
    {
    case 1:
    printf("Monday\n");
    break;
    case 2:
    printf("Tesday\n");
    break;
    case 3:
    printf("Wensday\n");
    break;
    case 4:
    printf("Thusday\n");
    break;
    case 5:
    printf("Friday\n");
    break;
    case 6:
    printf("Satarday\n");
    break;
    case 7:
    printf("Sunday\n");
    break;
    default:
    printf("Enter a valid Input");
    break;
    }
    return 0;
}
